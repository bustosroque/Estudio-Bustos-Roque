# Google Calendar API Configuration - Service Account
# IMPORTANTE: Reemplaza estos valores con tus credenciales reales

# Email del Service Account (del archivo JSON descargado)
GOOGLE_SERVICE_ACCOUNT_EMAIL=tu-service-account@tu-proyecto.iam.gserviceaccount.com

# Private Key del Service Account (del archivo JSON descargado)
# IMPORTANTE: Mantén las comillas dobles y los \n
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nMIIEvQIBADANBgkqhkiG9w0BAQEFAASCBKcwggSjAgEAAoIBAQC...\n-----END PRIVATE KEY-----\n"

# Project ID de Google Cloud
GOOGLE_PROJECT_ID=tu-proyecto-id

# Client ID del Service Account
GOOGLE_CLIENT_ID=tu-client-id

# Calendar ID del estudio (puede ser 'primary' para el calendario principal)
GOOGLE_CALENDAR_ID=primary

# Google Sheets ID para guardar leads automáticamente
# Obtén el ID de la URL de tu hoja: https://docs.google.com/spreadsheets/d/ID_AQUI/edit
GOOGLE_SHEET_ID=tu-sheet-id-aqui

# Email del estudio para recibir notificaciones
STUDIO_EMAIL=estudiojuridicobustosroque@gmail.com

# Configuración de la aplicación
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Telegram (opcional - para recibir leads)
TELEGRAM_BOT_TOKEN=tu_bot_token
TELEGRAM_CHAT_ID=tu_chat_id 